<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Color cuando el botón está presionado -->
    <item android:state_pressed="true" android:color="@android:color/holo_orange_light" />
    <!-- Color cuando el botón está deshabilitado -->
    <item android:state_enabled="false" android:color="@color/darkGray" />
    <!-- Color predeterminado cuando el botón no está presionado -->
    <item android:color="@color/sacaLaBiciYellow" />
</selector>
